{
  "name": "kendy-react-babel-boilerplate",
  "version": "1.0.0",
  "description": "My React app bootstrap project for quick development",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && http-server ./dist",
    "build:dll": "cross-env NODE_ENV=development webpack --progress --config internals/webpack/dll.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config internals/webpack/production.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config internals/webpack/development.config.js",
    "analyzer": "cross-env ANALYZING=true npm run build",
    "lint": "eslint --ext .jsx --ext .js --fix .",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "prettier": "prettier --write src/**/*.{scss,js,jsx,json}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhanhoangtrong/kendy-react-babel-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "immutable",
    "boilerplate",
    "javascript",
    "JSX",
    "webpack"
  ],
  "author": "Kendy Hoang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhanhoangtrong/kendy-react-babel-boilerplate/issues"
  },
  "homepage": "https://github.com/nhanhoangtrong/kendy-react-babel-boilerplate#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "connected-react-router": "^6.3.2",
    "core-js": "^3.6.5",
    "history": "^5.0.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-fastclick": "^3.0.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.0.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sanitize.css": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.0.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.1",
    "jest": "^27.0.6",
    "nib": "^1.1.2",
    "path-is-inside": "^1.0.2",
    "postcss": "^7.0.2",
    "postcss-autoreset": "^2.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.13.2",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^5.0.9",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "jest": {
    "setupFilesAfterEnv": ["<rootDir>/internals/jest/setupTests.js"],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "@app(.*)$": "<rootDir>/src$1"
    }
  }
}
